class Nodo:
    def _init_(self, dato):
        self.dato = dato
        self.siguiente = None

class ListaEnlazada:
    def _init_(self):
        self.cabeza = None

    def insertar_al_principio(self, dato):
        nuevo_nodo = Nodo(dato)
        nuevo_nodo.siguiente = self.cabeza
        self.cabeza = nuevo_nodo

    def imprimir_lista(self):
        actual = self.cabeza
        while actual:
            print(actual.dato, end=" -> ")
            actual = actual.siguiente
        print("None")

    def eliminar_en_medio(self, valor):
        actual = self.cabeza
        previo = None

        while actual:
            if actual.dato == valor:
                if previo:
                    previo.siguiente = actual.siguiente
                else:
                    self.cabeza = actual.siguiente
                return
            previo = actual
            actual = actual.siguiente

# Ejemplo de uso
lista = ListaEnlazada()
lista.insertar_al_principio(6)
lista.insertar_al_principio(3)
lista.insertar_al_principio(7)
lista.insertar_al_principio(11)
lista.insertar_al_principio(5) 
lista.imprimir_lista()

lista.eliminar_en_medio(7)
lista.imprimir_lista()
