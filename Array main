using namespace std;



int main() {
	int A[] = { {1}, {3}, {5}, {7}, {9} };
	Array array1(5);
	Array array2(5, 2);
	Array array3(5);
	Array array4(5);
	Array array5(5);
	array1.MostrarArray(A);
	int mayorNro = array1.MostrarMayor(A);
	int menorNro = array1.MostrarMenor(A);
	double promedio = array1.MostrarPromedio(A);
	cout << "El mayor numero del array 1 es" << mayorNro<<endl;

	cout << "El menor numero del array 1 es" << menorNro << endl;

	cout << "El promedio del array 1 es" << promedio << endl;

	return 0;

}
